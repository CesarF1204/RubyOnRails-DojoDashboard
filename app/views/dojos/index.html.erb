<h1>Listing <%= @dojos.count %> Dojos</h1>
<a href="/dojos/new">New Dojo</a>
<table>
    <thead>
        <tr>
            <th>Coding Dojo Branch</th>
            <th>Street</th>
            <th>City</th>
            <th>State</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    <% @dojos.each do |dojo| %>
        <tr>
            <td><%= dojo.branch %></td>
            <td><%= dojo.street %></td>
            <td><%= dojo.city %></td>
            <td><%= dojo.state %></td>
            <td>
                <a href="#">Show</a>
                <a href="#">Edit</a>
                <a href="#">Delete</a>
            </td>
        </tr>
    <% end %>
    </tbody>
</table>
